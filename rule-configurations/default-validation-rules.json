{
  "validation": {
      "rules": {
          "input-data-checks": {
          },
          "output-data-checks": {
              "matching-file-extensions": "png",
              "file-size-check": true
          },
          "color-checks": {
              "threshold": 1.2
          },
          "pixel-checks": {
            "resolution-allow-divergence": 0.5,
            "image-similarity-measures": 0.95,
            "resize-factor": 0.10
          },
          "classification-checks": {
            "matching-boundaries": 0.20,
            "resize-factor": 0.10
          },
          "nan-value-check": {
              "allow-nan": true,
              "correct": 0
          },
          "cluster-checks": {
            "deviation": 1
          }
      }
  }
}